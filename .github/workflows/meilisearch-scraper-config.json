{
  "index_uid": "strapi-docs",
  "start_urls": [
    "https://docs.strapi.io/cms/",
    "https://docs.strapi.io/cloud/"
  ],
  "sitemap_urls": [
    "https://docs.strapi.io/sitemap.xml"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".breadcrumbs__item:nth-child(3) .breadcrumbs__link",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": "h1",
    "lvl2": "h2", 
    "lvl3": "h3",
    "text": "article p, article li, main p, main li"
  },
  "selectors_exclude": [
    ".navbar",
    ".sidebar", 
    ".menu",
    ".breadcrumbs",
    ".pagination-nav",
    ".table-of-contents",
    ".hash-link",
    "nav",
    "aside"
  ],
  "custom_settings": {
    "rankingRules": [
      "words",
      "attribute",
      "proximity", 
      "exactness",
      "typo",
      "sort"
    ],
    "searchableAttributes": [
      "hierarchy_lvl1",
      "hierarchy_lvl0",
      "hierarchy_lvl2",
      "hierarchy_lvl3",
      "content"
    ],
    "displayedAttributes": [
      "hierarchy_lvl0",
      "hierarchy_lvl1", 
      "hierarchy_lvl2",
      "hierarchy_lvl3",
      "content",
      "url",
      "anchor"
    ]
  }
}