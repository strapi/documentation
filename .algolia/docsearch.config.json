{
  "index_name": "docs-v3",
  "start_urls": [
    "https://docs-v3.strapi.io"
  ],
  "selectors": {
    "lvl0": {
      "selector": "(//*[contains(@class, 'sidebar-heading open')])[1]",
      "type": "xpath",
      "global": true
    },
    "lvl1": {
      "selector": "//h1/text()",
      "type": "xpath",
      "global": true
    },
    "lvl2": {
      "selector": "/html/head/meta[@name='description']/@content",
      "type": "xpath",
      "global": true
    },
    "lvl4": {
      "selector": "//h2/text()",
      "type": "xpath",
      "global": true
    },
    "text": ".content__default p",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    },

    "filters": {
      "selector": "/html/head/meta[@name='filters']/@content",
      "type": "xpath",
      "global": true
    }

  },
  "strip_chars": " .,;:#",
  "stop_urls": [
    "https://docs-v3.strapi.io/developer-docs/latest/update-migration-guides/migration-guides"
  ],
  "custom_settings": {
    "attributesForFaceting": [
      "lang"
    ]
  }
}
